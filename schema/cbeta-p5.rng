  <define name="att.placement.attribute.place">
    <optional>
      <attribute name="place">
        <a:documentation>
: 1] below; 2] bottom; 3] margin; 4] top; 5] opposite; 6] overleaf; 7] above; 8] end; 9] inline; 10] inspace</a:documentation>
        <list>
          <oneOrMore>
            <choice>
              <value>below</value>
              <a:documentation>below the line</a:documentation>
              <value>bottom</value>
              <a:documentation>at the foot of the page</a:documentation>
              <value>margin</value>
              <a:documentation>in the margin (left, right, or both)</a:documentation>
              <value>top</value>
              <a:documentation>at the top of the page</a:documentation>
              <value>opposite</value>
              <a:documentation>on the opposite, i.e. facing, page</a:documentation>
              <value>overleaf</value>
              <a:documentation>on the other side of the leaf</a:documentation>
              <value>above</value>
              <a:documentation>above the line</a:documentation>
              <value>end</value>
              <a:documentation>at the end of e.g. chapter or volume.</a:documentation>
              <value>inline</value>
              <a:documentation>within the body of the text.</a:documentation>
              <value>inspace</value>
              <a:documentation> in a predefined space, for example left by an earlier scribe.</a:documentation>
              <data type="Name"/>
            </choice>
          </oneOrMore>
        </list>
