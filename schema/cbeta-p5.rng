        <choice>
          <data type="double"/>
          <data type="decimal"/>
        </choice>
      </attribute>
    </optional>
  </define>
  <define name="att.dimensions.attribute.precision">
    <optional>
      <attribute name="precision">
        <a:documentation>characterizes the precision of the values specified by the
      other attributes.</a:documentation>
        <choice>
          <value>high</value>
          <value>medium</value>
          <value>low</value>
          <value>unknown</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <define name="att.dimensions.attribute.scope">
    <optional>
